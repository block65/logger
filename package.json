{
  "name": "@block65/logger",
  "version": "12.0.0",
  "private": false,
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./sentry": "./lib/sentry.js",
    "./express": "./lib/express.js",
    "./lambda": "./lib/lambda.js"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "sentry": [
        "./lib/sentry.d.ts"
      ],
      "express": [
        "./lib/express.d.ts"
      ],
      "lambda": [
        "./lib/lambda.d.ts"
      ]
    }
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "CREDITS",
    "LICENSE.md"
  ],
  "scripts": {
    "bazel": "bazel",
    "bazel:buildifier": "bazel run //:buildifier",
    "bazel:format": "find . -type f \\( -name \"*.bzl\" -or -name WORKSPACE -or -name BUILD -or -name BUILD.bazel \\) ! -path \"*/node_modules/*\" ! -path \"*/tmp/*\" | xargs buildifier -v --warnings=attr-cfg,attr-license,attr-non-empty,attr-output-default,attr-single-file,constant-glob,ctx-actions,ctx-args,depset-iteration,depset-union,dict-concatenation,duplicated-name,filetype,git-repository,http-archive,integer-division,load,load-on-top,native-build,native-package,out-of-order-load,output-group,package-name,package-on-top,positional-args,redefined-variable,repository-name,same-origin-load,string-iteration,unsorted-dict-items,unused-variable",
    "bazel:lint": "yarn bazel:format --lint=warn",
    "bazel:lint-fix": "yarn bazel:format --lint=fix",
    "benchmark": "node --loader=ts-node/esm lib/benchmarks/basic.ts",
    "build": "bazel build",
    "build:clean": "bazel clean",
    "build:watch": "ibazel build",
    "test": "bazel test //:unit",
    "test:dev": "TEST_TARGET=DONOTUSE NODE_OPTIONS=--experimental-vm-modules jest --watch --runInBand",
    "test:dev:update": "TEST_TARGET=DONOTUSE NODE_OPTIONS=--experimental-vm-modules jest --runInBand --updateSnapshot",
    "test:update": "bazel run //:unit.update",
    "test:watch": "ibazel test //:unit"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.97",
    "@types/express": "^4.17.13",
    "@types/fast-redact": "^3.0.1",
    "colorette": "^2.0.16",
    "emittery": "^0.11.0",
    "fast-redact": "^3.1.1",
    "quick-format-unescaped": "^4.0.4",
    "serialize-error": "^11.0.0",
    "stream-chain": "^2.2.5"
  },
  "devDependencies": {
    "@bazel/bazelisk": "^1.11.0",
    "@bazel/buildifier": "^5.1.0",
    "@bazel/ibazel": "^0.16.2",
    "@bazel/typescript": "^5.4.2",
    "@block65/eslint-config": "^7.0.4",
    "@jest/globals": "^28.1.0",
    "@sentry/integrations": "^6.19.7",
    "@sentry/node": "^6.19.7",
    "@types/aws-lambda": "^8.10.97",
    "@types/fast-redact": "^3.0.1",
    "@types/node": "^16",
    "@types/quick-format-unescaped": "^4.0.0",
    "@types/stream-chain": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "pretty-format": "^28.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "type-fest": "^2.12.2",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "@sentry/integrations": "^7",
    "@sentry/node": "^7"
  },
  "peerDependenciesMeta": {
    "@sentry/integrations": {
      "optional": true
    },
    "@sentry/node": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
